{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Monster",
  "type": "object",
  "required": ["id", "name", "createdAt", "updatedAt"],
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "notes": {
      "type": "string"
    },
    "moduleIds": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "createdAt": {
      "type": "number"
    },
    "updatedAt": {
      "type": "number"
    }
  }
}
