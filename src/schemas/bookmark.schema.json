{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Bookmark",
  "type": "object",
  "required": ["id", "moduleId", "anchorId", "title", "createdAt", "updatedAt"],
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique identifier for the bookmark"
    },
    "moduleId": {
      "type": "string",
      "description": "ID of the module this bookmark belongs to"
    },
    "anchorId": {
      "type": "string",
      "description": "Anchor ID of the heading/bookmark"
    },
    "title": {
      "type": "string",
      "description": "Title of the heading/bookmark"
    },
    "createdAt": {
      "type": "number",
      "description": "Timestamp when the bookmark was created"
    },
    "updatedAt": {
      "type": "number",
      "description": "Timestamp when the bookmark was last updated"
    }
  }
} 